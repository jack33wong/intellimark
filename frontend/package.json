{
  "name": "intellimark-chat-frontend",
  "version": "1.0.0",
  "description": "Frontend for Intellimark Chat application",
  "private": true,
  "dependencies": {
    "@stripe/react-stripe-js": "^3.9.2",
    "@stripe/stripe-js": "^7.9.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.6.0",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "firebase": "^12.2.1",
    "katex": "^0.16.22",
    "lucide-react": "^0.294.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-katex": "^3.1.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^6.8.1",
    "react-scripts": "5.0.1",
    "rehype-katex": "^7.0.1",
    "remark-math": "^6.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:5001",
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "@types/react-router-dom": "^5.3.3",
    "typescript": "^4.9.5"
  },
"jest": {
    "transformIgnorePatterns": [
      "/node_modules/(?!react-markdown|vfile|unist-util-visit-parents|unist-util-visit|remark-rehype|rehype-raw|hast-util-to-jsx-runtime|remark-math|rehype-katex|mdast-util-to-hast|micromark|micromark-extension-gfm|mdast-util-gfm|mdast-util-from-markdown|decode-named-character-reference|character-entities)/"
    ],
    "moduleNameMapper": {
      "unist-util-visit-parents/do-not-use-color": "<rootDir>/node_modules/unist-util-visit-parents/index.js",
      "#minpath": "<rootDir>/node_modules/vfile/lib/minpath.js",
      "#minproc": "<rootDir>/node_modules/vfile/lib/minproc.js",
      "#minurl": "<rootDir>/node_modules/vfile/lib/minurl.js",
      "../config/firebase": "<rootDir>/src/config/__mocks__/firebase.ts"
    }
  }
}
